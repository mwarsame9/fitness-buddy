<!-- <h1>Workouts</h1> -->

<div id="index_workouts">
  <table>
    <tr>
      <th>Date</th>
      <th>Workout</th>
    </tr>
  <% @workouts.each do |workout| %>
    <tr>
      <td><%= link_to workout.date.strftime("%A, %B %d"), workout %></td>
      <td><%= workout.workout %></td>
    </tr>
    <% end %>
    </table>
</div>

<hr>

<%= link_to "New Workout", new_workout_path %>
